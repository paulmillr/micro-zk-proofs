{
  "name": "micro-zk-proofs",
  "version": "0.2.0",
  "description": "Create & verify zero-knowledge SNARK proofs in parallel, using noble cryptography",
  "files": [
    "*.js",
    "*.js.map",
    "*.d.ts",
    "*.d.ts.map",
    "src"
  ],
  "dependencies": {
    "@noble/curves": "^2.0.0-beta.2",
    "@noble/hashes": "^2.0.0-beta.4",
    "micro-packed": "^0.8.0-beta.1",
    "micro-wrkr": "^0.1.0"
  },
  "devDependencies": {
    "@paulmillr/jsbt": "0.4.4",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  },
  "scripts": {
    "build": "tsc",
    "build:release": "npx --no @paulmillr/jsbt esbuild test/build",
    "format": "prettier --write src test",
    "test": "node test/index.js",
    "test:deno": "deno test/index.js"
  },
  "keywords": [
    "groth16",
    "plonk",
    "zk",
    "snark",
    "stark",
    "zk-proof",
    "proof",
    "bn254",
    "zero-knowledge",
    "nizk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulmillr/micro-zk-proofs.git"
  },
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "author": "Paul Miller (https://paulmillr.com)",
  "license": "MIT"
}
